{
  "name": "task-management-grpc",
  "version": "1.0.0",
  "description": "gRPC clone of a Task Management REST API",
  "main": "dist/src/server/index.js",
  "scripts": {
    "proto:gen": "mkdir -p src/proto && grpc_tools_node_protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out=service=grpc-node,mode=grpc-js:./src/proto --js_out=import_style=commonjs,binary:./src/proto --grpc_out=grpc_js:./src/proto ./proto/*.proto",
    "build": "npm run proto:gen && tsc",
    "start": "node dist/src/server/index.js",
    "client": "node dist/src/client/index.js",
    "test": "jest",
    "dev": "ts-node src/server/index.ts"
  },
  "keywords": [
    "grpc",
    "task-management",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@grpc/grpc-js": "^1.9.13",
    "@grpc/proto-loader": "^0.7.10",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.3.1",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.7",
    "grpc-tools": "^1.12.4",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
